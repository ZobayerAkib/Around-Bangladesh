<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg5"
    tools:context=".chittagong">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp">


            <TextView
                android:layout_marginTop="30dp"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:fontFamily="casual"
                android:gravity="center"
                android:textStyle="bold"
                android:text="Chittagong"
                android:layout_marginBottom="40dp"
                android:textColor="@color/white"
                android:textSize="20dp">

            </TextView>
            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:cardCornerRadius="15dp"
                android:layout_margin="5dp"
                android:layout_gravity="center"
                android:elevation="10dp">

                <ImageView
                    android:id="@+id/im1"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:layout_gravity="center"
                    android:background="@drawable/saint" />
            </androidx.cardview.widget.CardView>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:fontFamily="@font/baloo_thambi"
                android:text="Saint Martin"
                android:textColor="@color/black"
                android:gravity="center"
                android:layout_marginBottom="30dp"
                android:textSize="15dp" />


            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:cardCornerRadius="15dp"
                android:layout_margin="5dp"
                android:layout_gravity="center"

                android:elevation="10dp">
                <ImageView

                    android:id="@+id/im2"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:layout_gravity="center"
                    android:background="@drawable/saj" />
            </androidx.cardview.widget.CardView>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:fontFamily="@font/baloo_thambi"
                android:text="Sajek Valley"
                android:textColor="@color/black"
                android:gravity="center"
                android:layout_marginBottom="30dp"
                android:textSize="15dp" />


            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:cardCornerRadius="15dp"
                android:layout_margin="5dp"
                android:layout_gravity="center"
                android:elevation="10dp">
                <ImageView

                    android:id="@+id/im3"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:layout_gravity="center"
                    android:background="@drawable/foy" />
            </androidx.cardview.widget.CardView>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:fontFamily="@font/baloo_thambi"
                android:text="Foy's Lake"
                android:textColor="@color/black"
                android:gravity="center"
                android:layout_marginBottom="30dp"
                android:textSize="15dp" />


            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:cardCornerRadius="15dp"
